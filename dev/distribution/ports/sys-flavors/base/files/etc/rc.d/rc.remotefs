#!/bin/bash

source /etc/rc.d/functions

function start()
{
write_message "Mounting remote filesystems..."
/bin/mount -a -v -t nfs > /dev/tty9 2> /dev/tty9
check_success $?
}

function stop
{
write_message "Unmounting remote filesystems..."
/bin/umount -a -f -tnfs > /dev/tty9 2> /dev/tty9
check_success $?


}

$1


